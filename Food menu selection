#include<stdio.h>
int main()
{
	int num;
	printf("Enter a number from 1 to 5:");
	scanf("%d",&num);
	
 switch(num)
 {
 	case 1: printf("Food item-Sandwich\nPrize-149\n");
 	break;
 	case 2: printf("Food item-Pasta\nPrize-179\n");	
	break;
 	case 3: printf("Food item-Pizza\nPrize-239\n");	
	break;
 	case 4: printf("Food item-French Fries\nPrize-99\n");	
	break;
 	case 5: printf("Food item-Burger\nPrize-129\n");
	break;
 	default: printf("Wrong number, please enter a number from 1 to 5");
 }
 return 0;
}
